<ResourceDictionary
    xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
    xmlns:local="using:FoodPlanner.WinUI">

    <!--Icon characters for arrow buttons-->
    <x:String x:Key="UpArrow">&#x02C4;</x:String>
    <x:String x:Key="DownArrow">&#x02C5;</x:String>

    <x:String x:Key="WeekPlanHeader">Veckans mat</x:String>
    
    <x:String x:Key="Monday">må:</x:String>
    <x:String x:Key="Tuesday">ti:</x:String>
    <x:String x:Key="Wednesday">on:</x:String>
    <x:String x:Key="Thursday">to:</x:String>
    <x:String x:Key="Friday">fr:</x:String>
    <x:String x:Key="Saturday">lö:</x:String>
    <x:String x:Key="Sunday">sö:</x:String>
    

</ResourceDictionary>
